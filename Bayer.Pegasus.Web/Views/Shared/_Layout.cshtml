<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Bayer.Pegasus.Web</title>

    <link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/lib/bootstrap-select/dist/css/bootstrap-select.min.css" rel="stylesheet">

    <!-- DataTable.NET CSS -->
    <link href="~/lib/DataTables/datatables.min.css" media="all" rel="stylesheet" type="text/css" />
    <link href="~/lib/datatables/Buttons-1.5.2/css/buttons.bootstrap4.min.css" rel="stylesheet" />
    <link href="~/lib/datatables/Buttons-1.5.2/css/buttons.dataTables.min.css" rel="stylesheet" />
    <link href="~/lib/datatables/DataTables-1.10.18/css/dataTables.bootstrap4.min.css" rel="stylesheet" />
    <link href="~/lib/datatables/DataTables-1.10.18/css/jquery.dataTables.min.css" rel="stylesheet" />

    <!-- JQuery UI -->
    <link href="~/lib/jquery-ui/themes/base/jquery-ui.min.css" rel="stylesheet" />

    <!-- MetisMenu CSS -->
    <link href="/css/metisMenu.min.css" rel="stylesheet">

    <!-- Token Field CSS -->
    <link href="~/lib/bootstrap-tokenfield/dist/css/bootstrap-tokenfield.min.css" rel="stylesheet" />
    <link href="~/lib/bootstrap-tokenfield/dist/css/tokenfield-typeahead.min.css" rel="stylesheet" />

    <!-- Simple Bar CSS -->
    <link href="~/lib/simplebar/dist/simplebar.css" rel="stylesheet" />


    <!-- Custom CSS -->
    <link href="/css/sb-admin-2.css" rel="stylesheet">

    <link href="/css/cdn-datatables-css/jquery.dataTables.min.css" rel="stylesheet">
    <link href="/css/cdn-datatables-css/buttons.dataTables.min.css" rel="stylesheet">
    <link href="/css/cdn-datatables-css/responsive.dataTables.min.css" rel="stylesheet">
    <link href="/css/cdn-datatables-css/select.dataTables.min.css" rel="stylesheet">

    <!-- Custom Fonts -->
    <link href="~/lib/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">

    <link href="~/lib/eonasdan-bootstrap-datetimepicker/build/css/bootstrap-datetimepicker.min.css" rel="stylesheet" />

    <link rel="stylesheet" href="~/css/pegasus.css" />
    <link rel="stylesheet" href="~/css/bayer-controls.css" />

    <link href="~/lib/toastr/toastr.min.css" rel="stylesheet" />

</head>
<body>

    <div id="wrapper">
        <div id="page-wrapper" class="container-fluid">
            @RenderBody()
            @{ Html.RenderPartial("ModalForms.cshtml"); }
        </div>
        <!-- /#page-wrapper -->

        <div class="modal fade in" id="modalWindow" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="false">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                        <h4 class="modal-title" id="myModalLabel">Alerta</h4>
                    </div>
                    <div class="modal-body">
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default" data-dismiss="modal">Fechar</button>

                    </div>
                </div>
                <!-- /.modal-content -->
            </div>
            <!-- /.modal-dialog -->
        </div>


        <div class="modal fade in" id="modalWindowSucess" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="false">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" style="margin-right: 25px;display: block;float: right;" data-dismiss="modal" aria-hidden="true">×</button>
                        <h4 class="modal-title" id="myModalLabelSucesso" style="padding-left: 20px;">Detalhamento</h4>
                    </div>
                    <div class="modal-body" style="padding-left: 20px;">
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default" data-dismiss="modal">Fechar</button>

                    </div>
                </div>
                <!-- /.modal-content -->
            </div>
            <!-- /.modal-dialog -->
        </div>
    </div>


    <!--Base64-->
    <script src="/js/base64.js"></script>
    <!-- jQuery -->
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/jquery-ui/jquery-ui.min.js"></script>
    <script src="~/lib/jquery-ui/jquery-ui.min.js"></script>
    <script src="~/lib/jquery-mask/jquery.mask.js"></script>

    <!-- cdn-datatables -->
    @*<script src="~/lib/cdn-datatables/jquery-2.2.0.min.js"></script>*@
    <script src="~/lib/cdn-datatables/jquery.dataTables.min.js"></script>
    <script src="~/lib/cdn-datatables/dataTables.buttons.min.js"></script>
    <script src="~/lib/cdn-datatables/buttons.flash.min.js"></script>
    <script src="~/lib/cdn-datatables/buttons.html5.min.js"></script>
    <script src="~/lib/cdn-datatables/dataTables.responsive.min.js"></script>
    <script src="~/lib/cdn-datatables/dataTables.select.min.js"></script>
    <script src="~/lib/cdn-datatables/dataTables.checkboxes.min.js"></script>
    

    <!-- Bootstrap Core JavaScript -->
    <script src="~/lib/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="~/lib/bootstrap-select/dist/js/bootstrap-select.min.js"></script>

    <script src="~/lib/knockout/dist/knockout.js"></script>
    <script src="~/lib/knockout-mapping/knockout.mapping.js"></script>
    <script src="~/lib/knockout-dragdrop/lib/knockout.dragdrop.js"></script>
    <script src="~/lib/simplebar/dist/simplebar.js"></script>

    <!-- Token Field -->
    <script src="~/lib/typeahead.js/dist/typeahead.jquery.min.js"></script>
    <script src="~/lib/bootstrap-tokenfield/dist/bootstrap-tokenfield.min.js"></script>
    <script src="~/lib/moment/min/moment-with-locales.min.js"></script>

    <!-- <!-- DataTable.NET JavaScript -->
    @*<script src="~/lib/DataTables/datatables.min.js" type="text/javascript"></script>
    <script src="~/lib/datatables/DataTables-1.10.18/js/jquery.dataTables.min.js" type="text/javascript"></script>
    <script src="~/lib/datatables/DataTables-1.10.18/js/dataTables.bootstrap4.min.js" type="text/javascript"></script>
    <script src="~/lib/datatables/Buttons-1.5.2/js/dataTables.buttons.min.js" type="text/javascript"></script>
    <script src="~/lib/datatables/Buttons-1.5.2/js/buttons.bootstrap4.min.js" type="text/javascript"></script>
    <script src="~/lib/datatables/pdfmake-0.1.36/pdfmake.min.js" type="text/javascript"></script>
    <script src="~/lib/datatables/datatables.defaults.js" type="text/javascript"></script>
    <script src="~/lib/datatables/DataTables-1.10.18/js/datetime-moment.js" type="text/javascript"></script>
    <script src="~/lib/datatables/DataTables-1.10.18/js/formatted-numbers.js" type="text/javascript"></script>*@


    <!-- Plugin Format Money-->
    <script src="~/lib/accounting/accounting.min.js"></script>

    <!-- Plugin Gráficos -->
    <script src="~/js/flot/jquery.flot.min.js"></script>
    <script src="~/js/flot/jquery.flot.resize.min.js"></script>
    <script src="~/js/flot/plugins/jquery.flot.tooltip.js"></script>
    <script src="~/js/flot/plugins/jquery.flot.growraf.js"></script>
    <script src="~/js/flot/plugins/jquery.flot.axislabels.js"></script>
    <script src="~/js/flot/plugins/jquery.flot-valuelabels.js"></script>

    <script src="~/lib/eonasdan-bootstrap-datetimepicker/build/js/bootstrap-datetimepicker.min.js"></script>

    <environment include="Development">
        <script src="~/lib/typeahead.js/dist/bloodhound.js"></script>
        <script src="~/lib/knockoutjs-tokenfield/dist/knockoutjs-tokenfield.js"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/lib/typeahead.js/dist/bloodhound.min.js"></script>
        <script src="~/lib/knockoutjs-tokenfield/dist/knockoutjs-tokenfield.min.js"></script>
    </environment>

    <!-- Metis Menu Plugin JavaScript -->
    <script src="/js/metisMenu.min.js"></script>
    <!-- DataTables JavaScript -->
    <!-- Custom Theme JavaScript -->
    <script src="/js/sb-admin-2.js"></script>
    <script src="/js/validationResults.js"></script>
    <!-- jQuery -->
    <script src="/js/util.js"></script>

    <script src="/js/ajaxFlow.js"></script>
    <!-- Custom -->

    <script src="~/lib/toastr/toastr.min.js"></script>

    <script src="~/js/ViewModels/Shared/ReportDateViewModel.js"></script>
    <script src="~/js/ViewModels/Shared/HistoryViewModel.js"></script>
    <script src="~/js/pegasus.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
