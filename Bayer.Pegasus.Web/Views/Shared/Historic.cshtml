
<div id="ultimas-analises" class="row panel-filtros" data-bind="with:HistoryViewModel">
    <h2 class="title-tab" data-bind="visible:Items().length>0">Últimas Análises</h2>
    <h2 class="title-tab" data-bind="visible:Items().length==0">Nenhuma Análise Registrada</h2>
    <div class="col-lg-12 formContent">
        <div class="panel panel-default">
            <div class="panel-body">
                <div class="row">
                    <div class="col-lg-12 table-responsive">
                        <table class="table table-hover">
                            <thead data-bind="visible:Items().length>0">
                                <tr>
                                    <th></th>
                                    <th class="text-center">Data</th>
                                </tr>
                            </thead>
                            <tbody data-bind="foreach: Items">
                             

                                <tr >
                                    <td data-bind="attr: {'data-id': id}, click: $parent.LoadHistoric.bind($data, json)"><a href="#"><i class="fa fa-calendar-o"></i><span data-bind="text:description"></span></a></td>
                                    <td class="text-center" data-bind="attr: {'data-id': id}, click: $parent.LoadHistoric.bind($data, json),text:created"></td>
                                    <td style="width:5%"><a data-bind="click:$parent.Delete"><i class="fa fa-trash-o"></i></a></td>
                                </tr>

                            </tbody>
                        </table>
                    </div>
                </div>
                <!-- /.row (nested) -->
            </div>
            <!-- /.panel-body -->
        </div>
        <!-- /.panel -->
    </div>
    <!-- /.col-lg-12 -->
</div>
<!-- /.row -->