@model Bayer.Pegasus.Web.Models.GenericReportModel
<div class="wrap-row">
    <div class="col-md-12">
        <table id="DatatableCFOPRegistration" class="table table-striped table-bordered dt-responsive cell-border row-border" style="width:100%">
            <thead>
                <tr>
                    <th></th>
                    <th>ID Arquivo</th>
                    <th>Arquivo</th>
                    <th>Status</th>
                    <th>Ação</th>
                    <th>Data da Ação</th>
                </tr>
            </thead>
        </table>
    </div>
</div>

<div class="modal fade" id="modalCFOP" role="document">
    <div class="modal-dialog modal-lg">
        <div class="modal-content" style="padding-left: 20px;">
            <div class="modal-header">
                <button id="buttonModalCFOP" type="button" class="close" data-dismiss="modal" data-bind="click: $root.CreateOrUpdateCFOP" aria-label="Close" style="width:5%;">
                    <span aria-hidden="true">×</span>
                </button>
                <h4 class="modal-title" style="width:90%;"><span class="fa fa-edit" aria-hidden="true"></span> <span data-bind="text: TitleModal"></span></h4>
            </div>
            <div class="modal-body" hidden="hidden">
                <form>
                    <div class="form-group" aria-hidden="true">
                        <label style="width:90%;">Selecione:</label>
                    </div>
                    <div class="form-group" aria-hidden="true">
                        <label style="width:90%;">ID Arquivo:</label>
                        <input type="text" class="form-control small-padding" id="idArquivoretroativo" placeholder="" style="width:15%;" maxlength="4" data-bind="value: idArquivoretroativo">
                        <input type="hidden" id="idArquivoretroativohidden" name="acao" value="" data-bind="value: idArquivoretroativoHidden">

                    </div>
                    <div class="form-group" aria-hidden="true">
                        <label style="width: 90%;">Status:</label>
                        <input type="text" class="form-control small-padding" id="dsAcao" placeholder="" style="width: 90%;" data-bind="value: dsAcao">
                    </div>
                    <div class="form-group" aria-hidden="true">
                        <label style="width: 90%;">Arquivo:</label>
                        <input type="text" class="form-control small-padding" id="dsNome" placeholder="" style="width: 90%;" data-bind="value: dsNome">
                    </div>

                </form>
            </div>
            <div class="modal-footer" hidden="hidden">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancelar</button>
                <button type="button" class="btn btn-cyan" data-bind="click: $root.CreateOrUpdateCFOP"><i class="fa fa-angle-double-right" aria-hidden="true"></i><span>Gravar</span></button>
            </div>

        </div>
    </div>
</div>

<div class="modal fade" id="modalCFOP1" role="document">
    <div class="modal-dialog modal-lg">
        <div class="modal-content" style="padding-left: 20px;">
            <div class="modal-header">
                <button id="buttonModalCFOP1" type="button" class="close" data-dismiss="modal" data-bind="click: $root.CreateOrUpdateCFOP1" aria-label="Close" style="width:5%;">
                    <span aria-hidden="true">×</span>
                </button>
                <h4 class="modal-title" style="width:90%;"><span class="fa fa-edit" aria-hidden="true"></span> <span data-bind="text: TitleModal"></span></h4>
            </div>
            <div class="modal-body" hidden="hidden">
                <form>
                    <div class="form-group" aria-hidden="true">
                        <label style="width:90%;">Código Cfop1:</label>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>


<div class="modal fade" id="modalCFOP2" role="document">
    <div class="modal-dialog modal-lg">
        <div class="modal-content" style="padding-left: 20px;">
            <div class="modal-header">
                <button id="buttonModalCFOP2" type="button" class="close" data-dismiss="modal" data-bind="click: $root.CreateOrUpdateCFOP2" aria-label="Close" style="width:5%;">
                    <span aria-hidden="true">×</span>
                </button>
                <h4 class="modal-title" style="width:90%;"><span class="fa fa-edit" aria-hidden="true"></span> <span data-bind="text: TitleModal"></span></h4>
            </div>
            <div class="modal-body" hidden="hidden">
                <form>
                    <div class="form-group" aria-hidden="true">
                        <label style="width:90%;">Código Cfop2:</label>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>