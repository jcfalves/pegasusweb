@model Bayer.Pegasus.Web.Models.GenericReportModel
<div id="filtro-consulta" class="row panel-filtros">
    <div class="col-lg-12 title-row">
        <h2 id="titleFiltro" runat="server" class="title-tab col-md-6" visible="false">Filtro de Consulta</h2>
    </div>
    <div class="col-lg-12 formContent">
        <div class="panel panel-default">
            <div class="panel-body">
                <div class="row">
                    <div class="col-lg-12">
                        <form role="form">

                            <div class="halfside col-md-12">
                                @{ Html.RenderPartial("FilterContent", Model); }
                            </div>
                        </form>
                    </div>

                </div>
                <!-- /.row (nested) -->
            </div>
            <!-- /.panel-body -->
        </div>
        <!-- /.panel -->
    </div>
    <!-- /.col-lg-12 -->
</div>
<!-- /.row -->
