            <div class="panel-body">
                <div class="row">
                    <div class="col-lg-12">
                        <form role="form">
                            <div class="halfside col-md-12">
                                <div class="wrap-row">
                                    <div class="container">
                                        <div class="form-group">
                                            <div class="loader"></div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="halfside col-md-12">
                                @{ Html.RenderPartial("FilterContent"); }
